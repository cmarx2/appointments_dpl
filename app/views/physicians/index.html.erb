<h1>Physicians</h1>
<div class="ui three stackable cards">
<% @physicians.each do |physician|%>
<a class="card" href="<%=physician_path(physician)%>">
  <div class="content">
    <div class="header">Physician</div>
    <div class="meta">
      <span class="category"><%=physician.dr_name%></span>
    </div>
    <div class="description">
      <p>Click your name to view appointments and patients</p>
    </div>
  </div>
  <div class="extra content">
    <div class="right floated author">
      <img class="ui avatar image" src="https://course_report_production.s3.amazonaws.com/rich/rich_files/rich_files/2475/s300/beaker-white-256.png"> 
    </div>
  </div>
</a>
<%end%>
</div>



